          <eXo:OCLScript ID="dd778127-54ae-4b79-9917-242ba3705ef5" Name="defaultname">
            <eXo:Contents><![CDATA[/* choice tests - choice between attributes is not possible to 
define sufficiently in XSD */

/* either open tournament or belongs to some league */

context Tournament
inv: qualification.choice_1.child_OpenTournament.open = true or 
qualification.choice_1.child_League.leagueName <> null 

/* now some other methods how to rewrite previous constraint
to demonstrate choice naming and PSM navigation */


context Tournament
inv: qualification.choice_1.child_1.open = true or 
qualification.choice_1.child_2.leagueName <> null 
/*
context Tournament
inv: 
qualification.child(CH_Qualification_OpenTournament_League).child(1).open = true or 
qualification.child(CH_Qualification_OpenTournament_League).child(2).leagueName <> null 

inv:
qualification.CH_Qualification_OpenTournament_League.OpenTournament.open = true or 
qualification.CH_Qualification_OpenTournament_League.League.leagueName <> null 
*/]]></eXo:Contents>
          </eXo:OCLScript>
        </eXo:OCLScripts>
